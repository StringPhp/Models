{
    "name": "stringphp/models",
    "description": "A collection of helpers for creating abstractions of data models in PHP.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StringPhp\\Models\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\StringPhp\\Models\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Robert Snedeker",
            "email": "rsnedeker20@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "stringphp/utils": "^v0.0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "pestphp/pest": "^2.34",
        "fakerphp/faker": "^1.23"
    },
    "scripts": {
        "fix": "php-cs-fixer fix --using-cache=no",
        "fix:dry": "php-cs-fixer fix --using-cache=no --diff --dry-run",
        "test": "pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
